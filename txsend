#!/bin/bash
cd ~/scaletest/
source coinlist
count=0

if [[ $1 = "" ]]
then
 echo "no transaction size specified"
 exit
fi

while [ "x${coinlist[count]}" != "x" ]
do
  all=${coinlist[count]}
  name=${all%% *}
  if [ "$name" != "" ]
    then
       size=$(komodo-cli -ac_name=$name getmempoolinfo | awk ' /\"size\"/ {printf $2}' | sed 's/,//' )
