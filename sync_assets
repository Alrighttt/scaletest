#!/bin/bash
source seedip
source coinlist
source pubkey.txt
count=0
delay=1

while [ "x${coinlist[count]}" != "x" ]
do
  all=${coinlist[count]}
  name=${all%% *}
  supply=`expr "$all" : '.* \(.*\)'`
  if [ "$name" != "" ]
    then
     ~/komodo/src/komodod -pubkey=$pubkey -ac_cc=1 -ac_name=$name -ac_supply=$supply -ac_reward=100000000 -addnode=$seed_ip $1 &
     sleep $delay
  fi
  count=$(( $count +1 ))
done
