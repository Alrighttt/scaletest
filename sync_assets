#!/bin/bash
source seedip
source coinlist
source pubkey.txt
count=0
delay=5

while [ "x${coinlist[count]}" != "x" ]
do
  all=${coinlist[count]}
  name=${all%% *}
  supply=`expr "$all" : '.* \(.*\)'`
  if [ "$name" != "" ]
    then
     komodod -ac_name=$name -ac_supply=$supply -addnode=$seed_ip -pubkey=$pubkey $1 &
     sleep $delay
  fi
  count=$(( $count +1 ))
done
