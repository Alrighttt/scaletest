#!/bin/bash
delay=1

source seedip
source coinlist2
source pubkey.txt
count=0
while [[ $count -le ${#coinlist2[@]} ]]
do
  all=${coinlist2[count]}
  name=${all%% *}
  supply=`expr "$all" : '.* \(.*\)'`
  if [ "$name" != "" ]
    then
     ~/komodo/src/komodod -pubkey=$pubkey -ac_cc=3 -ac_name=$name -ac_supply=$supply -ac_reward=100000000 $1 -addnode=$seed_ip &
     sleep $delay
  fi
  count=$(( $count +1 ))
done
