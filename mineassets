#!/bin/bash
source seedip
source coinlist
count=0
delay=5
while [ "x${coinlist[count]}" != "x" ]
do
  all=${coinlist[count]}
  name=${all%% *}
  supply=`expr "$all" : '.* \(.*\)'`
  if [ "$name" != "" ]
    then
     komodod -ac_name=$name -ac_supply=$supply -addnode=$seed_ip -gen $args &
     sleep $delay
    fi
  count=$(( $count +1 ))
done
